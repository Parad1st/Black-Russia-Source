<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="#CB191919"/>
            <corners android:radius="@dimen/_100sdp"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <scale android:scaleWidth="100%">
            <shape>
                <gradient android:startColor="#5A5A5A" android:endColor="#7C7C7C" android:angle="45.0" android:centerX="79.99999%" android:centerColor="#8F8F8F"/>
                <stroke android:width="1.4dp" android:color="#CB191919"/>
                <corners android:radius="@dimen/_100sdp"/>
            </shape>
        </scale>
    </item>
</layer-list>