            android:layout_marginEnd="69dp"
            app:layout_constraintBottom_toBottomOf="@+id/slap_button"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/slap_button"
            app:layout_constraintTop_toTopOf="@+id/slap_button"
            app:layout_constraintVertical_bias="1.0"
            app:srcCompat="@drawable/admin_red" />

        <TextView
            android:id="@+id/textView30"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/muller_bold"
            android:text="BAN"
            android:textColor="#FFFFFF"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="@+id/ban_button"
            app:layout_constraintEnd_toEndOf="@+id/ban_button"
            app:layout_constraintStart_toStartOf="@+id/ban_button"
            app:layout_constraintTop_toTopOf="@+id/ban_button" />

