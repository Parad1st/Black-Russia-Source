            android:text="[0]  "
            android:textColor="#FFFFFF"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="@+id/nickname_fon"
            app:layout_constraintEnd_toStartOf="@+id/nickname"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/nickname_fon" />

        <TextView
            android:id="@+id/nickname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/muller_bold"
            android:text="Parad1st_Top"
            android:textColor="#FFFFFF"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="@+id/nickname_fon"
            app:layout_constraintEnd_toEndOf="@+id/nickname_fon"
